{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "typechain": "typechain --target=ethers-v5 --always-generate-overloads --out-dir src/typechain \"src/**/*.json\""
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "timeswap-v1-sdk",
  "author": "Emmanuel Antony",
  "module": "dist/timeswap-v1-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/timeswap-v1-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/timeswap-v1-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@typechain/ethers-v5": "^7.0.1",
    "ethers": "^5.4.6",
    "husky": "^7.0.1",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^5.1.2",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/constants": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.5",
    "tiny-invariant": "^1.1.0"
  }
}
