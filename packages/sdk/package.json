{
  "version": "0.1.5",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "gentype": "yarn gentype:contracts && yarn gentype:abi",
    "gentype:contracts": "typechain --target=ethers-v5 --out-dir src/typechain/timeswap './node_modules/@timeswap-labs/timeswap-v1-?(core|convenience)/artifacts/contracts/**/Timeswap?(Pair|Factory|Convenience).json'",
    "gentype:abi": "typechain --target=ethers-v5 --out-dir src/typechain/abi './src/abi/*.json'"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@timeswap-labs/timeswap-v1-sdk",
  "author": "Emmanuel Antony",
  "module": "./dist/timeswap-v1-sdk.esm.js",
  "repository": "git://github.com/Timeswap-Labs/Timeswap-V1-SDK.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "size-limit": [
    {
      "path": "./dist/timeswap-v1-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "./dist/timeswap-v1-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@timeswap-labs/timeswap-v1-convenience": "1.0.6",
    "@timeswap-labs/timeswap-v1-core": "1.0.6",
    "@typechain/ethers-v5": "^7.0.1",
    "ethers": "^5.4.6",
    "husky": "^7.0.1",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^5.1.2",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/abstract-provider": "^5.4.1",
    "@timeswap-labs/timeswap-v1-sdk-core": "0.1.7",
    "tiny-invariant": "^1.1.0"
  }
}
